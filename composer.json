{
    "name": "silvershop/silverstripe-hasonefield",
    "description": "Edit a has_one relationship using a GridField, triggered by clicking a button.",
    "license": "BSD-2-Clause",
    "type": "silverstripe-vendormodule",
    "require": {
        "php": "^8.3",
        "silverstripe/vendor-plugin": "^3",
        "silverstripe/framework": "^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "SilverShop\\HasOneField\\": "src/",
            "SilverShop\\HasOneField\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "5.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "hchokshi/silverstripe-hasonefield": "*"
    }
}
